Package: python-pecan
Architecture: all
Pre-Depends: dpkg (>= 1.15.6~)
Depends: python (<< 2.8),
         python (>= 2.6.6-7~),
         python-logutils,
         python-mako (>= 0.4.0),
         python-markupsafe,
